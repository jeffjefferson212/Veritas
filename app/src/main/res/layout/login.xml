<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/loginPageBackground"
        android:layout_width="451dp"
        android:layout_height="775dp"
        android:background="@drawable/loginpagebackgroud"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="321dp"
        android:layout_height="550dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="78dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="116dp"
        android:background="@drawable/loginborder"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="143dp"
        android:layout_height="142dp"
        android:layout_marginStart="134dp"
        android:layout_marginTop="98dp"
        android:layout_marginEnd="137dp"
        android:layout_marginBottom="504dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/usericon" />

    <Button
        android:id="@+id/button2"
        android:layout_width="247dp"
        android:layout_height="51dp"

        android:layout_marginStart="82dp"
        android:layout_marginTop="469dp"
        android:layout_marginEnd="85dp"
        android:layout_marginBottom="224dp"
        android:background="@drawable/loginbutton"
        android:text="login"

        android:textColor="@android:color/white"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="276dp"
        android:layout_height="50dp"
        android:layout_marginStart="67dp"
        android:layout_marginTop="299dp"
        android:layout_marginEnd="71dp"
        android:layout_marginBottom="395dp"
        android:background="@drawable/logininput"
        android:ems="10"
        android:gravity="center"
        android:hint="Email"
        android:inputType="textPersonName"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="276dp"
        android:layout_height="50dp"
        android:layout_marginStart="67dp"
        android:layout_marginTop="382dp"
        android:layout_marginEnd="71dp"
        android:layout_marginBottom="312dp"
        android:background="@drawable/logininput"
        android:ems="10"
        android:gravity="center"
        android:hint="Password"
        android:inputType="textPersonName"
        android:password="true"

        android:textColor="@android:color/darker_gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="Deprecated" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="49dp"
        android:layout_height="49dp"
        android:layout_marginStart="82dp"
        android:layout_marginTop="299dp"
        android:layout_marginEnd="283dp"
        android:layout_marginBottom="396dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_dialog_email" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="49dp"
        android:layout_height="49dp"
        android:layout_marginStart="82dp"
        android:layout_marginTop="383dp"
        android:layout_marginEnd="283dp"
        android:layout_marginBottom="312dp"
        android:tint="@android:color/holo_red_dark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_lock_lock" />

    <Button
        android:id="@+id/button3"
        android:layout_width="135dp"
        android:layout_height="18dp"
        android:layout_marginStart="138dp"
        android:layout_marginTop="532dp"
        android:layout_marginEnd="138dp"
        android:layout_marginBottom="194dp"
        android:background="@android:color/white"
        android:stateListAnimator="@null"
        android:text="Forgot Password"
        android:textAllCaps="false"
        android:textColor="@android:color/darker_gray"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/returnHomeXButton"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="366dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="662dp"
        android:background="@android:drawable/presence_offline"
        android:stateListAnimator="@null"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>